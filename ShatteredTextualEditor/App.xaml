<Application x:Class="ShatteredTextualEditor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ShatteredTextualEditor"
			 DispatcherUnhandledException="Application_DispatcherUnhandledException"
             Startup="Application_Startup">
    <Application.Resources>
		<SolidColorBrush x:Key="TransparentWhite" Color="White" Opacity="0.75"/>
		<SolidColorBrush x:Key="LightGrey" Color="#C8C5C5"/>
	</Application.Resources>
</Application>
